.pagetitle.mainpage__title.pagetitle__withoutborder
  .layout-line
    %h1.pagetitle__title Платиновые спонсоры
.mainpage__sponsors
  .layout-line
    #platin_sponsors_logos.company__logos.logos_carousel.touchcarousel
      %ul.touchcarousel-container
        %li.touchcarousel-item
          %div
          = link_to "http://ibs.ru/", target: "_blank" do
            = image_tag "logos/ibs.jpg"
        %li.touchcarousel-item
          %div
          = link_to "https://www.machinezone.com/", target: "_blank" do
            = image_tag "logos/machine_zone.png"
        %li.touchcarousel-item
          %div
          = link_to "http://mobirate.com/", target: "_blank" do
            = image_tag "logos/mobirate.png"
